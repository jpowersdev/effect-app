{
  "name": "effect-app",
  "license": "MIT",
  "description": "Effect boilerplate for building web applications",
  "homepage": "https://github.com/jpowersdev/effect-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpowersdev/effect-app"
  },
  "bugs": {
    "url": "https://github.com/jpowersdev/effect-app/issues"
  },
  "tags": [
    "typescript",
    "effect-ts",
    "boilerplate"
  ],
  "keywords": [
    "typescript",
    "effect-ts",
    "boilerplate"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "provenance": true
  },
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "main": "dist/main.js",
  "packageManager": "pnpm@9.0.4",
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build --plugins annotate-pure-calls --out-dir build --source-maps",
    "check": "tsc -b tsconfig.json",
    "dev": "tsx --watch --env-file=.env src/main.ts",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "clean": "node scripts/clean.mjs",
    "circular": "node scripts/circular.mjs",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "docgen": "pnpm exec docgen && node scripts/docs.mjs",
    "dtslint": "pnpm run dtslint",
    "dtslint-clean": "dtslint --installAll",
    "changeset-version": "changeset version && node scripts/version.mjs",
    "changeset-publish": "pnpm build && TEST_DIST= pnpm vitest && changeset publish"
  },
  "resolutions": {
    "dependency-tree": "^10.0.9",
    "detective-amd": "^5.0.2",
    "detective-cjs": "^5.0.1",
    "detective-es6": "^4.0.1",
    "detective-less": "^1.0.2",
    "detective-postcss": "^6.1.3",
    "detective-sass": "^5.0.3",
    "detective-scss": "^4.0.3",
    "detective-stylus": "^4.0.0",
    "detective-typescript": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.8",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@edge-runtime/vm": "^4.0.0",
    "@effect/build-utils": "^0.7.7",
    "@effect/docgen": "^0.4.3",
    "@effect/dtslint": "^0.1.0",
    "@effect/eslint-plugin": "^0.2.0",
    "@effect/language-service": "^0.1.0",
    "@effect/platform-node": "^0.59.0",
    "@effect/vitest": "^0.6.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/browser": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/expect": "^1.6.0",
    "@vitest/web-worker": "^1.6.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-codegen": "^0.28.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "fast-check": "^3.20.0",
    "glob": "^11.0.0",
    "madge": "^7.0.0",
    "playwright": "^1.45.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@effect/platform": "^0.64.0",
    "@effect/schema": "^0.72.3",
    "effect": "^3.7.2",
    "eslint": "^8"
  },
  "pnpm": {
    "patchedDependencies": {
      "babel-plugin-annotate-pure-calls@0.4.0": "patches/babel-plugin-annotate-pure-calls@0.4.0.patch"
    },
    "updateConfig": {
      "ignoreDependencies": [
        "eslint"
      ]
    }
  }
}
